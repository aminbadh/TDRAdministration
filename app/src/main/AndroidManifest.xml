<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aminbadh.tdradministrationlpm">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TDRAdministration">
        <activity
            android:name=".activity.StudentActivity"
            android:parentActivityName=".activity.DetailActivity" />
        <activity
            android:name=".activity.DetailActivity"
            android:launchMode="singleTop"
            android:parentActivityName=".activity.RegistrationActivity" />
        <activity
            android:name=".activity.RegistrationActivity"
            android:launchMode="singleTop"
            android:parentActivityName=".activity.ClassActivity" />
        <activity
            android:name=".activity.ClassActivity"
            android:launchMode="singleTop"
            android:parentActivityName=".activity.LevelActivity" />
        <activity
            android:name=".activity.LevelActivity"
            android:launchMode="singleTop" />
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>